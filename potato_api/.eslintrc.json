{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-unused-vars":"off",
        "max-len":"off",
        "arrow-body-style":"off",
        "no-use-before-define":"off"
    }
}
